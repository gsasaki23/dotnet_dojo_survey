@using dojo_survey.Models;
@model User;

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dojo Survey Index</title>
</head>
<body>
    <h2>Submitted Info</h2>
    <div id="left" style="display:inline-block;">
        <h3>Name:</h3>
        <h3>Dojo Location:</h3>
        <h3>Favorite Language:</h3>
        <h3>Comment</h3>
    </div>
    <div id="right" style="display:inline-block;">
        <h3>@Model.FullName</h3>
        <h3>@Model.Location</h3>
        <h3>@Model.Language</h3>
        <h3>@Model.Comment</h3>
    </div>
    <a href="/" style="display:block;">Go Back</a>
</body>
</html>